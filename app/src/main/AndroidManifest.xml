<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.meethere">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the "MyLocation" functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".sharedpreferences.App"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MeetHere"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.SelectDestinationActivity"
            android:exported="true" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyB0cxRl8WstOzAISge8W_w4k8oq-rx9AJ8" />

        <activity
            android:name=".activity.WebViewActivity"
            android:exported="true"
            android:label="장소 검색하기"
            android:theme="@style/Search" />
        <activity
            android:name=".activity.SetLocationNew"
            android:exported="true"
            android:label="장소 검색하기"
            android:parentActivityName=".activity.MainActivity"
            android:theme="@style/Search" />
        <activity
            android:name=".activity.SplashActivity"
            android:exported="true"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/ID" />
        <activity
            android:name=".activity.RegisterActivity"
            android:label="회원가입"
            android:parentActivityName=".activity.LoginActivity"
            android:theme="@style/Register" />
        <activity
            android:name=".activity.FindIDActivity"
            android:label="아이디 찾기"
            android:parentActivityName=".activity.LoginActivity"
            android:theme="@style/Register" />
        <activity
            android:name=".activity.FindPWActivity"
            android:label="비밀번호 찾기"
            android:parentActivityName=".activity.LoginActivity"
            android:theme="@style/Register" />
        <activity
            android:name=".activity.AuthIDActivity"
            android:label="이메일 인증"
            android:parentActivityName=".activity.RegisterActivity"
            android:theme="@style/Register" />
        <activity
            android:name=".activity.EditActivity"
            android:label="정보 수정"
            android:parentActivityName=".activity.MainActivity"
            android:theme="@style/Register" />
        <activity
            android:name=".activity.BookmarkActivity"
            android:label="즐겨찾기"
            android:parentActivityName=".activity.MainActivity"
            android:theme="@style/Register" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:theme="@style/NoActionBarStyle" />
        <activity
            android:name=".activity.SelectDestination_2_6Activity"
            android:label="목적지 선택"
            android:parentActivityName=".activity.SetLocationNew"
            android:theme="@style/Search" />
        <activity
            android:name=".activity.ShowResult_2_7Activity"
            android:label="검색결과"
            android:parentActivityName=".activity.SelectDestination_2_6Activity"
            android:theme="@style/Search" />
        <activity
            android:name=".activity.ShowDetail_2_8Activity"
            android:label="상세보기"
            android:parentActivityName=".activity.ShowResult_2_7Activity"
            android:theme="@style/Search" />
        <activity
            android:name=".activity.ShowDetailMap_2_9Activity"
            android:label="지도보기"
            android:parentActivityName=".activity.ShowDetail_2_8Activity"
            android:theme="@style/Search" />
    </application>

</manifest>